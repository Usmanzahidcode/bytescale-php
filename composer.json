{
  "name": "usmanzahid/bytescale-php",
  "description": "An open source php package for bytescale cloud storage and files processing.",
  "keywords": [
    "files",
    "cloud",
    "storage",
    "bytescale",
    "php",
    "media"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "UsmanZahid\\Bytescale\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UsmanZahid\\Bytescale\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Usman Zahid",
      "email": "developerusman@yahoo.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^12"
  }
}
